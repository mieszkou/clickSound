; -------------------------------------------------------------------------------------------
; | MP3 from memory
; -------------------------------------------------------------------------------------------


_Example()



Func _Example()
	Local $bMP3 = _MP3()
	Local $bMP3_2 = _MP3_2()
	Local $aWav = _DSnd_MP3Decode($bMP3)
	Local $aWav_2 = _DSnd_MP3Decode($bMP3_2)

	Local $oDS = _DSnd_Create()
	Local $oDS_Buffer = _DSnd_CreateSoundBuffer($oDS, BitOR($DSBCAPS_GLOBALFOCUS, $DSBCAPS_CTRLFREQUENCY), BinaryLen($aWav[0]), $aWav[1].Channels, $aWav[1].SamplesPerSec, $aWav[1].BitsPerSample, $aWav[1].FormatTag)
	Local $oDS_Buffer_2 = _DSnd_CreateSoundBuffer($oDS, BitOR($DSBCAPS_GLOBALFOCUS, $DSBCAPS_CTRLFREQUENCY), BinaryLen($aWav_2[0]), $aWav_2[1].Channels, $aWav_2[1].SamplesPerSec, $aWav_2[1].BitsPerSample, $aWav_2[1].FormatTag)

	Local $aLock = _DSnd_BufferLock($oDS_Buffer)
	Local $tLock = DllStructCreate("byte[" & $aLock[1] & "];", $aLock[0])
	DllStructSetData($tLock, 1, $aWav[0])
	_DSnd_BufferUnLock($oDS_Buffer, $aLock)

	$aLock = _DSnd_BufferLock($oDS_Buffer_2)
	$tLock = DllStructCreate("byte[" & $aLock[1] & "];", $aLock[0])
	DllStructSetData($tLock, 1, $aWav_2[0])
	_DSnd_BufferUnLock($oDS_Buffer_2, $aLock)
	$oDS_Buffer_2.SetFrequency($aWav_2[1].SamplesPerSec * 1.2)


	Local $aBuffer[4][3]
	For $i=0 To UBound($aBuffer)-1
		$aBuffer[$i][0] = _DSnd_DuplicateSoundBuffer($oDS, $oDS_Buffer)
		$aBuffer[$i][1] = TimerInit()
		$aBuffer[$i][2] = Random(1, 8, 1) * 250
	Next

	Local $fSRdivFREQ = $aWav[1].SamplesPerSec / 523.251
	Local $iBaseNote, $fFreq
	SplashTextOn("Cat on Piano", "Cat on Piano" & @CRLF & "<ESC> to stop", 300, 100)
	While Not _IsPressed("1B")
		$iBaseNote = Random(0, 12, 1)

		For $i=0 To UBound($aBuffer)-1
			If TimerDiff($aBuffer[$i][1]) >= $aBuffer[$i][2] Then
				$fFreq = 440 * 2 ^ (($iBaseNote + Random(-12, 12, 1)) / 12)

				$aBuffer[$i][1] = TimerInit()
				If Random(0, 4, 1) = 0 Then $aBuffer[$i][2] = Random(1, 8, 1) * 250
				$aBuffer[$i][0].Stop()
				$aBuffer[$i][0].SetCurrentPosition(0)
				$aBuffer[$i][0].SetFrequency($fSRdivFREQ * $fFreq)
				_DSnd_BufferPlay($aBuffer[$i][0])
			EndIf
		Next

		Sleep(10)
	WEnd
	SplashOff()

	_DSnd_BufferPlay($oDS_Buffer_2)
	Sleep(1500)
	$oDS_Buffer_2.Stop()

	For $i=0 To UBound($aBuffer)-1
		$aBuffer[$i][0].Stop()
		$aBuffer[$i][0] = 0
	Next

	$oDS_Buffer_2 = 0
	$oDS_Buffer = 0
	$oDS = 0
EndFunc   ;==>_Example







